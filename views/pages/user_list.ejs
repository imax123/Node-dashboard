<% include ../partials/head %> 
<% include ../partials/header %> 
<% include ../partials/menu %>
      <!--main content start-->
      <section id="main-content">
          <section class="wrapper site-min-height">
          	<div class="row mt">
          		<div class="col-lg-12">
          		 <div class="row mt">
				  <div class="col-md-12">
				      <div class="content-panel">
				          <table class="table table-striped table-advance table-hover">
					  	  	  <h4><i class="fa fa-angle-right"></i> User List</h4>
					  	  	  <hr>
				              <thead>
				              <tr>
				                  <th><i class="fa fa-user"></i>First Name</th>
				                  <th><i class="fa fa-user"></i>Last Name</th>
				                  <th class="hidden-phone"><i class="fa fa-mobile"></i>Mobile</th>
				                  <th><i class="fa fa-envelope"></i>Email</th>
				                  <th><i class=" fa fa-edit"></i>Options</th>
				                  <th></th>
				              </tr>
				              </thead>
				              <tbody>
				              <% items.forEach(function(item) { %>
				              <tr>
				                  <td><%= item.fname %></td>
				                  <td><%= item.lname %></td>
				                  <td class="hidden-phone"><%= item.mobile %></td>
				                  <td><%= item.email %></td>
				                  <td>
				                      <button class="btn btn-success btn-xs"><i class="fa fa-check"></i></button>
				                      <button class="btn btn-primary btn-xs"><i class="fa fa-pencil"></i></button>
				                      <button class="btn btn-danger btn-xs"><i class="fa fa-trash-o "></i></button>
				                  </td>
				              </tr>
				              <% }); %>
				            </tbody>
				          </table>
				      </div><!-- /content-panel -->
				  </div><!-- /col-md-12 -->
			      </div><!-- /row -->
			  		</div>
			  	</div>
			
		</section><! --/wrapper -->
      </section><!-- /MAIN CONTENT -->

      <!--main content end-->
<% include ../partials/footer %>
